    <script>
        function CBcheck(id) {
            if ($("#" + id + "")[0].checked == true) {
                document.getElementById(id + "Table").style.display = "block";
            }
            else {
                document.getElementById(id + "Table").style.display = "none";
            }
        }

        function CBcheckALL(id) {
            if ($("#" + id + "")[0].checked == true) {
                for (var i = 0; i < 100; i++) {
                    $("#SameRoomSet_" + i).attr("checked", true);
                }
            }
            else {
                for (var i = 0; i < 100; i++) {
                    $("#SameRoomSet_" + i).attr("checked", false);
                }
            }
        }

        function BLCBcheck(id) {
            if ($("#" + id + "")[0].checked == true) {
                document.getElementById(id + "Table").style.display = "block";

                document.getElementById("IsRecordViolate").checked = $("#" + id + "")[0].checked;
                document.getElementById("UseBlacklist").checked = $("#" + id + "")[0].checked;

                // $("#IsRecordViolate").attr("checked", true); //IsRecordViolate
                // $("#UseBlacklist").attr("checked", true); //UseBlacklist
            }
            else {
                document.getElementById(id + "Table").style.display = "none";
            }
        }

        var SeatSelectDefaultMode = @ViewBag.SeatSelectDefaultMode;
        var roomSetPosTimesIsUsed = '@ViewBag.SeatSelectPosChecked';
        var SelectSeatPosCountText = @ViewBag.SelectSeatPosCountText;
        var SelectSeatPosTimesText = @ViewBag.SelectSeatPosTimesText;

        var SeatSelectAdMode =  '@ViewBag.SelectSeatPosTimesText';
        //设置选座模式
        $(document).ready(function () {
            $("input[name='SeatSelectDefaultMode'][value='"+SeatSelectDefaultMode+"']").attr("checked",true); //设置选座模式
            if(roomSetPosTimesIsUsed=='True'){     //设置是否启用选座次数限制
                $("#SeatSelectPos").attr("checked",'true');
            }else{
                $("#SeatSelectPos").removeAttr("checked");
            }
            $("#SelectSeatPosTimes").val(SelectSeatPosTimesText);//设置限制多少分钟内
            $("#SelectSeatPosCount").val(SelectSeatPosCountText);//设置限制多少分钟内刷卡选座次数

            if(SeatSelectAdMode=='True'){     //设置是否启用选座模式高级设置
                $("#SeatSelectAdMode").attr("checked",'true');
            }else{
                $("#SeatSelectAdMode").removeAttr("checked");
            }

        });


